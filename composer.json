
{
	"name": "b13/online-media-updater",
	"description": "Update YouTube/Vimeo preview images",
	"type": "typo3-cms-extension",
	"license": [
		"GPL-2.0-or-later"
	],
	"require": {
		"typo3/cms-core": "^12.0",
		"typo3/cms-filelist": "^12.0",
		"typo3/cms-backend": "^12.0",
		"typo3/cms-fluid": "^12.0"
	},
	"autoload": {
		"psr-4": {
			"B13\\OnlineMediaUpdater\\": "Classes/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "online_media_updater",
			"web-dir": ".Build/Web"
		}
	},
	"require-dev": {
		"phpstan/phpstan": "^1.9",
		"typo3/coding-standards": "^0.7.1"
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin",
		"allow-plugins": {
			"typo3/cms-composer-installers": true,
			"typo3/class-alias-loader": true
		}
	}
}
